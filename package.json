{
  "name": "shopify-metabridge",
  "version": "0.1.0",
  "description": "CLI tool for syncing Shopify metaobject definitions, metafield definitions, and entries between stores",
  "main": "dist/index.js",
  "bin": {
    "metabridge": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "shopify",
    "metaobject",
    "metafield",
    "metabridge",
    "cli",
    "migration",
    "sync",
    "bridge",
    "admin-api"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "inquirer": "^12.10.0",
    "minimatch": "^10.0.3",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.12",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
